/boot emmc /dev/block/platform/soc/by-name/boot
/data ext4 /dev/block/platform/soc/by-name/userdata flags=fsflags=noatime,nosuid,nodev
#/data ext4 /dev/block/platform/soc/by-name/userdata flags=encryptable=/dev/block/platform/soc/by-name/metadata
/system ext4 /dev/block/platform/soc/by-name/system
/cache ext4 /dev/block/platform/soc/by-name/cache
/misc emmc /dev/block/platform/soc/by-name/para
/recovery emmc /dev/block/platform/soc/by-name/recovery flags=backup=1
/usb-otg auto /dev/block/sda1 /dev/block/sda flags=display="USB OTG";storage;wipeingui;removable
/external_sd vfat /dev/block/mmcblk1p1 /dev/block/mmcblk1 flags=display="Micro SDcard";storage;wipeingui;removable

